		var num = this.page_pos * this.max_size;
		for(var i = 0; i < this.max_size; i++) {
			if(i < this.cur_size) {
				obj = this.dataArray[num + i];
				$("search_result_list" + i + "_title").innerHTML = obj.name;
				$("search_result_list" + i + "_name").innerHTML = obj.singer_name;
				
				if("mv" == obj.type) {
					$("search_result_list" + i + "_img").src = "images/099.png";
				} else {
					$("search_result_list" + i + "_img").src = "images/tm.gif";
				}
				$("search_result_list" + i).style.visibility = "visible";
			} else {
				$("search_result_list" + i).style.visibility = "hidden";
			}
		}
		
		$("page_num").innerHTML = (this.page_pos + 1) + "/" + this.page_size;
	},
	
	time_out : 0,
	initFocusOn : function(_flag) {
		var num = this.page_pos * this.max_size + this.pos;
		var obj = this.dataArray[num];
		if(obj) {
			this.time_out = showTitleForMarquee(obj.name, $("search_result_list" + this.pos + "_title"), 11);
		}
		if(!_flag) {
			this.focusObj.style.width = "552px";
			this.focusObj.style.height = "112px";
			this.focusObj.style.left = "597px";
			if(checkSTBType()) {
				this.focusObj.style.webkitTransitionDuration = "350ms";
			}
		}
	},
	focusOn : function(_flag) {
		var num = this.page_pos * this.max_size + this.pos;
		var obj = this.dataArray[num];
		if(obj) {
			this.time_out = showTitleForMarquee(obj.name, $("search_result_list" + this.pos + "_title"), 11);
		}
		if(!_flag) {
			this.focusObj.style.visibility = "visible";
			this.focusObj.style.width = "552px";
			this.focusObj.style.height = "112px";
			this.focusObj.style.left = "597px";
			if(checkSTBType()) {
				this.focusObj.style.webkitTransitionDuration = "350ms";
			}
		}
	},
	
	focusOut : function(_flag) {
		clearTimeout(this.time_out);
		var num = this.page_pos * this.max_size + this.pos;
		$("search_result_list" + this.pos + "_title").innerHTML = this.dataArray[num].name;
		if(!_flag) {
			this.focusObj.style.visibility = "hidden";
			if(checkSTBType()) {
				this.focusObj.style.webkitTransitionDuration = "0ms";
			}
		}
	},
	
	focusMove : function(_num) {
		var num = _num || 0;
		var pos = this.pos + num;
		if(pos < 0) {
			this.changePage(-1);
		} else if(pos >= this.cur_size) {
			this.changePage(1);
		} else {
			this.focusOut(1);
			this.pos = pos;
			
			this.focusObj.style.top = 92 + 60 * this.pos + "px";
			this.focusOn(1);
		}
		$("prop").style.top = 61 + (this.pos + this.page_pos * this.max_size) * (500 / this.len) + "px";
	},
	
	changePage : function(_num) {
		var num = _num || 0;
		var pos = this.page_pos + num;
		if(pos >= 0 && pos < this.page_size) {
			this.pos = 0;
			this.page_pos = pos;
			
			this.show();
			this.focusMove();
		}
	},
	
	doSelect : function() {
		var backUrl = window.location.href.split("?")[0] + "?area=" + oparateObj.area + "&search_text="
            + searchObj.search_text + "&s_pos=" + this.pos + "&s_p_pos="
            + this.page_pos + "&backUrl=" + Q.encode(oparateObj.backUrl)
            +"&test="+paramObj.test+"&prid="+paramObj.prid+"&rid="+oparateObj.rid;
		var num = this.page_pos * this.max_size + this.pos;
		var obj = this.dataArray[num];
		//此处不再请求详情（请求鉴权）
        ajaxForAuth(obj,backUrl,function (authResult) {
            checkAndToPlay(authResult,backUrl,"","",paramObj.prid,paramObj.test);
        },function () {
            console.log("报错");
        });
	}
};

var oparateObj = {
	area : 0,
	search_flag : 0,
	
	backUrl : "",
	recommendId:1,
	panelId:11,
	pagelevel:2,
	init : function() {
		this.getParameter();
		searchObj.init();
		this.focusOn("init");
		searchObj.showSearchText(1);
		if("" == searchObj.search_text){
			initListData();
		}
		dcHTMLToReciveServlet(this.backUrl.split("?")[0], window.location.href.split("?")[0], "qq_music_search");
	},
	
	getParameter : function() {
		var search_text = Q.getDecoded("search_text");
		if(null != search_text && "null" != search_text) {
			searchObj.search_text = search_text;
		}
		var area = Q.getDecoded("area");
		if(null != area && "null" != area) {
			this.area = parseInt(area);
		}
		var s_pos = Q.getDecoded("s_pos");
		if(null != s_pos && "null" != s_pos) {
			searchResultListObj.pos = parseInt(s_pos);
		}
		var s_p_pos = Q.getDecoded("s_p_pos");
		if(null != s_p_pos && "null" != s_p_pos) {
			searchResultListObj.page_pos = parseInt(s_p_pos);
		}
		var h_pos = Q.getDecoded("h_pos");
		if(null != h_pos && "null" != h_pos) {
			hotSearchListObj.pos = parseInt(h_pos);
		}
		var h_p_pos = Q.getDecoded("h_p_pos");
		if(null != h_p_pos && "null" != h_p_pos) {
			hotSearchListObj.page_pos = parseInt(h_p_pos);
		}

        var rid = Q.getDecoded("rid");
        if(null != rid && "null" != rid) {
            this.rid = rid;
        }
		
		var backUrl = Q.getDecoded("backUrl");
		if(null != backUrl && "null" != backUrl) {
			this.backUrl = backUrl;
		}
	},
	
	focusOn : function(init) {
		switch(this.area) {
			case 0 :
				searchObj.focusOn();
				searchObj.focusMove();
				break;
			case 1 :
				if(init==="init"){
					searchResultListObj.initFocusOn();
				}else{
					searchResultListObj.focusOn();
				}
				searchResultListObj.focusMove();
				break;
			case 2 :
				if(init==="init"){
					hotSearchListObj.initFocusOn();
				}else{
					hotSearchListObj.focusOn();
				}
				hotSearchListObj.focusMove();
				break;
			case 10 :
				popUpObj.focusOn();
				break;
		}
	},
	
	focusOut : function() {
		switch(this.area) {
			case 0 :
				searchObj.focusOut();
				break;
			case 1 :
				searchResultListObj.focusOut();
				break;
			case 2 :
				hotSearchListObj.focusOut();
				break;
			case 10 :
				popUpObj.focusOut();
				break;
		}
	},
	
	moveX : function(_num) {
		switch(this.area) {
			case 0 :
				if(_num > 0 && ((0 == searchObj.area && 1 == searchObj.pos[0]) || (2 == searchObj.area && 1 == searchObj.pos[2]) || (1 == searchObj.area && searchObj.dataArray[searchObj.data_arr_pos].size_line - 1 == searchObj.pos[1] % searchObj.dataArray[searchObj.data_arr_pos].size_line))) {
					if(searchResultListObj.len < 1 && 1 == this.search_flag) {
						return;
					}
					this.focusOut();
					this.area = 0 == this.search_flag ? 2 : 1;
					this.focusOn();
				} else {
					searchObj.moveX(_num);
				}
				break;
			case 1 :
				if(_num < 0) {
					this.focusOut();
					this.area = 0;
					this.focusOn();
				}
				break;
			case 2 :
				if(_num < 0) {
					this.focusOut();
					this.area = 0;
					this.focusOn();
				}
				break;
			case 10 :
				popUpObj.focusMove(_num);
				break;
		}
	},
	
	moveY : function(_num) {
		switch(this.area) {
			case 0 :
				searchObj.moveY(_num);
				break;
			case 1 :
			 	searchResultListObj.focusMove(_num * searchResultListObj.size_line);
				break;
			case 2 :
				hotSearchListObj.focusMove(_num * hotSearchListObj.size_line);
				break;
			case 10 :
				
				break;
		}
	},
	
	changePage : function(_num) {
		if(1 == this.area) {
			searchResultListObj.changePage(_num);
		}
	},
	
	doSelect : function() {
		ajaxForPanelClick();
		switch(this.area) {
			case 0 :
				searchObj.doSelect();
				break;
			case 1 :
				searchResultListObj.doSelect();
				break;
			case 2 :
				hotSearchListObj.doSelect();
				break;
			case 10 :
				popUpObj.doSelect();
				break;
		}
	},
	
	doBack : function() {
		if(10 == this.area) {
			popUpObj.hidden();
		} else if(3 == searchObj.area) {
			searchObj.hiddenT9Focus(-1);
		} else {
			location.href = oparateObj.backUrl;
		}
	}
};

function init() {
	searchObj.focusObj.style.visibility = "hidden";
	initglobalParam();
	checkAndInitPress();
	oparateObj.init();
	if(checkSTBType()) {
		$("prop").style.webkitTransitionDuration = "350ms";
	}
}

window.onload = init;

function initListData(_keywords) {
	var keywords = _keywords || "";
	if("" != keywords) {
		ajaxForSearchProgram(_keywords);
	} else {
		oparateObj.focusOn("init");
		ajaxForHotSearchProgram();
	}
	paramObj.endtime = getCurrntTime();
	ajaxForExposure();
}


function ajaxForHotSearchProgram() {
	ajax({
		url: paramObj.serverUrl + "/hotSearch?rid="+oparateObj.rid+"&prid="+paramObj.prid+"&userId="
            + getCAcardNum()+"&test="+paramObj.test,// 请求的URL,可传参数
		type: "POST", //HTTP 请求类型,GET或POST
		onSuccess: function(html){ //请求成功后执行[可选]
			var json = eval('('+html+')');
			if("0" == json.code) {
				hotSearchListObj.init(json.data);
				oparateObj.focusOn();
				payBackPlay(hotSearchListObj);
				searchObj.focusObj.style.visibility = "visible";
			} else {
				
			}
		},
		onError:function(){ //请求失败后执行[可选]
			popUpObj.init("获取数据失败！");
		},
        onComplete:function(){// 请求成功.失败或完成(无论成功失败都会调用)时执行的函数[可选]
		   // alert("onComplete");	             			           		  
        },
        post:"", 
        timeout:7000  //请求的超时时间，默认7000ms，也可自行设定[可选]
	});
}

function ajaxForSearchProgram(_keywords) {
	var keywords = _keywords || "";
	ajax({
		url: paramObj.serverUrl + "/search?pinyin=" + keywords+"&rid="+oparateObj.rid+"&prid="+paramObj.prid+"&userId="
            + getCAcardNum()+"&test="+paramObj.test,// 请求的URL,可传参数
		type: "POST", //HTTP 请求类型,GET或POST
		onSuccess: function(html){ //请求成功后执行[可选]
			var json = eval('('+html+')');
			if("0" == json.code) {
				searchResultListObj.init(json.data);
				oparateObj.focusOn();
				payBackPlay(searchResultListObj);
				searchObj.focusObj.style.visibility = "visible";
			} else {
				
			}
		},
		onError:function(){ //请求失败后执行[可选]
			popUpObj.init("获取数据失败！");
		},
        onComplete:function(){// 请求成功.失败或完成(无论成功失败都会调用)时执行的函数[可选]
		   // alert("onComplete");	             			           		  
        },
        post:"", 
        timeout:7000  //请求的超时时间，默认7000ms，也可自行设定[可选]
	});
}
