                    <span   th:each="item,istat:${column.page_list}" class="item"
                         th:style="'position:absolute;z-index:4;top:'+${item.layout_start.split(' ')[1]}+'px;left:'+${item.absolute_y+65}+'px;width:'+${item.layout_end.split(' ')[0]}+'px;height:'+${item.layout_end.split(' ')[1]}+'px'"
