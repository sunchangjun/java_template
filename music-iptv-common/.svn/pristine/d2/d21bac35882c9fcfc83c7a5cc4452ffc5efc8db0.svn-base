            <div th:each="column,stat:${columns}"
                 th:id="'column'+${stat.index}+''"
                 th:title="''+${column.doMajor()}+'|'+${column.layout_regular}+'|'+${column.layout_line}+''"
                 th:style="'position: absolute;left: 0;top: '+${column.top+30}+'px;width:1280px;height:'+${column.column_height}+'px;z-index: 2'">
                <!--标题-->
                <div th:if="${column.layout_show_title}"  style="position: absolute;left: 0;top: 0;width:1280px;height: 45px;z-index: 3;">
                    <a  th:text="${column.name}" style="position:absolute;top: 0;left: 65px;height: 45px;width: 200px;font-size: 20px;z-index: 3;color: white;line-height: 45px;"></a>
                    <a  style="position:absolute;top: 0;right: 65px;width: 500px;line-height: 45px;text-align: right;" title="column_pages"></a>
                </div>
                <!--规则主题-->
                <div th:if="${column.layout_regular}"  th:style="'top:'+ @{(${column.layout_show_title} ? '45px' : '0px')} + ';width:1280px;z-index:3;position:absolute;'">
                    <span class="item"
                          th:each="item,istat:${column.page_list}"
                          th:style="'position:absolute;z-index:4;width:'+${item.width}+';height:'+${item.height}+';top:'+${item.absolute_x}+'px;left:'+${item.absolute_y+65}+'px'"
