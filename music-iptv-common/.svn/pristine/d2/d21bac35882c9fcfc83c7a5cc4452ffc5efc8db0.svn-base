                          th:id="'item'+${istat.index}+''">
                        <img th:src="${item.poster==null?item.bposter:item.poster}"
                             th:style="'width:'+${item.width}+';height:'+${item.height}+';z-index: 5;top:0;left:0;'"/>
                        <img th:src="@{/280.png}" th:style="'width:'+${item.width}+';position: absolute;left: 0;top:'+ ${item.resHeight - 50} +'px;height:50px;z-index: 6;'"/>
<!--                        <a th:text="${item.resTitle}" th:title="${item.resTitle}"  th:style="'height:23px;overflow: hidden;width:'+${item.resWidth-6}+'px;position: absolute;color:white;left: 3px;top: '+ ${item.resHeight - 27} +'px;z-index: 7;'" th:if="${item.type.toString()!='cate'}"></a>-->
<!--
                              <marquee   th:text="${item.name}" th:title="${item.name}"  th:style="'display:inline;visibility: visible;height:23px;overflow: hidden;width:'+${item.resWidth-6}+'px;position: absolute;color:white;left: 3px;top: '+ ${item.resHeight - 27} +'px;z-index: 7;'" th:if="${item.type.toString()!='cate'}"></marquee>
-->
                        <div th:style="'height: 25px;width:'+${item.resWidth-6}+'px;position: absolute;color:white;left: 3px;top: '+ ${item.resHeight - 27} +'px;z-index: 7;'" th:if="${item.type.toString()!='cate'}">
                            <marquee th:title="${item.name}"  style="height: 25px;vertical-align: bottom;visibility: hidden;font-size: 15px;margin: 0px;padding: 0px;border:0px;" th:text="${item.name}"> </marquee>
                            <a th:title="${item.resTitle}" style="height: 25px;vertical-align: bottom;position: absolute;top: 0px;left:0px;font-size:15px;" th:text="${item.resTitle}"> </a>
                        </div>
                    </span>
                    <script>
                        /*<![CDATA[*/
                        var columnId = 'column'+'[[${stat.index}]]';
                        var layout_line = '[[${column.layout_line}]]';
                        var type = '[[${column.child_type.toString()}]]';
                        /*]]>*/
                        var sum = layout_line==1?6:12;
                        if(type=='mv'){
                            sum = layout_line==1?4:8;
                        }
                        $(columnId).regular ="true";
                        $(columnId).sum =sum;
                        var imgs = $(columnId).getElementsByTagName("span");
                        for(var i=0;i<imgs.length;i++){
                            if(i>=sum){
                                imgs[i].style.display="none";
                            }
                        }
                    </script>
                </div>
                <!--不规则主体-->
