package hk.reco.music.iptv.common.wthx;

import java.io.Serializable;
import java.util.Date;

/**
 * @ClassName WthxTop
 * @Description TODO 万泰榜单
 * @Author wangpq
 * @Date 2019/5/17 11:38
 * @Version 1.0
 */
public class WthxTop implements Serializable {

    private static final long serialVersionUID = -6092600364313898497L;

    /**
     * 主键
     */
    private Long id;

    /**
     * 资源ID
     */
    private Long res_id;

    /**
     * 榜单类别 1歌曲2MV
     */
    private int top_type;

    /**
     * 榜单名称
     */
    private String top_name;

    /**
     *  小图地址
     */
    private String simg_url;

    /**
     * 大图地址
     */
    private String bimg_url;

    /**
     * 创建时间
     */
    private Date create_time;

    /**
     * 标签
     */
    private String wthx_tags;

    /**
     * 更新日期
     */
    private Date update_time;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Long getRes_id() {
        return res_id;
    }

    public void setRes_id(Long res_id) {
        this.res_id = res_id;
    }

    public int getTop_type() {
        return top_type;
    }

    public void setTop_type(int top_type) {
        this.top_type = top_type;
    }

    public String getTop_name() {
        return top_name;
    }

    public void setTop_name(String top_name) {
        this.top_name = top_name;
    }

    public String getSimg_url() {
        return simg_url;
    }

    public void setSimg_url(String simg_url) {
        this.simg_url = simg_url;
    }

    public String getBimg_url() {
        return bimg_url;
    }

    public void setBimg_url(String bimg_url) {
        this.bimg_url = bimg_url;
    }

    public Date getCreate_time() {
        return create_time;
    }

    public void setCreate_time(Date create_time) {
        this.create_time = create_time;
    }

    public String getWthx_tags() {
        return wthx_tags;
    }

    public void setWthx_tags(String wthx_tags) {
        this.wthx_tags = wthx_tags;
    }

    public Date getUpdate_time() {
        return update_time;
    }

    public void setUpdate_time(Date update_time) {
        this.update_time = update_time;
    }
}
