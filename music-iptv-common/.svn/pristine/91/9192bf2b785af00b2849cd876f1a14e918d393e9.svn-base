html, body, div, span,
h1, h2, h3, h4, h5, h6, p,
a, img, i, ol, ul, li,
form, label,
footer, header {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
}

ol, ul {
    list-style: none;
}

body {
    width: 1280px;
    height: 720px;
    position: relative;
    overflow: hidden;
    padding: 0;
    margin: 0;
    user-select: none;
}


.pull_left {
    float: left;
}

.pull_right {
    float: right;
}

.backBtn {
    width: 43px;
    vertical-align: middle;
    height: 43px;
}

.backTitle {
    font-size: 32px;
    font-family: Source Han Sans CN;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    line-height: 32px;
    margin-left: 15px;
    vertical-align: middle;
}
.baseStyle {
    display: inline-block;
    font-size: 14px;
    text-align: center;
}
/*访问样式*/
.rowhover {
    background: rgba(255, 255, 255, 0.2);
    font-weight: bold;
}


.y-tip {
    color: #FFD260;
}
