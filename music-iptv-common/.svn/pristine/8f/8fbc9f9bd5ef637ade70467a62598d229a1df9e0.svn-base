var oparateObj = {
	area : 1,
	rid : "",
	title : "",
    type :"",
	code : "",
	backUrl : "",
	recommendId:1,
    img:"",
	panelId:9,
	pagelevel:2,
	init : function() {
		this.getParameter();
		this.focusOn();
		initListData(this.rid, this.code);
		dcHTMLToReciveServlet(this.backUrl.split("?")[0], window.location.href.split("?")[0], "qq_music_prefecture");
	},
	
	getParameter : function() {
		var area = Q.getDecoded("area");
		if(null != area && "null" != area) {
			this.area = parseInt(area);
		}
		var l_pos = Q.getDecoded("l_pos");
		if(null != l_pos && "null" != l_pos) {
			listObj.pos = parseInt(l_pos);
		}
		var l_p_pos = Q.getDecoded("l_p_pos");
		if(null != l_p_pos && "null" != l_p_pos) {
			listObj.page_pos = parseInt(l_p_pos);
		}
		var code = Q.getDecoded("code");
		if(null != code && "null" != code) {
			this.code = code;
		}
		var rid = Q.getDecoded("rid");
		if(null != rid && "null" != rid) {
			this.rid = rid;
		}
		var title = Q.getDecoded("title");
		if(null != title && "null" != title) {
			this.title = title;
		}
		var backUrl = Q.getDecoded("backUrl");
		if(null != backUrl && "null" != backUrl) {
			this.backUrl = backUrl;
		}
        var type = Q.getDecoded("type");
        if(null != type && "null" != type) {
            this.type = type;
        }
        var img = Q.getDecoded("img");
        if(null != img && "null" != img) {
            $("pf_poster").src=img;
            this.img = img;
        }
	},
	
	focusOn : function() {
		switch(this.area) {
			case 0 :
				
				break;
			case 1 :
				listObj.focusOn();
				listObj.focusMove();
				break;
			case 2 :
				
				break;
			case 5 :
				listDoObj.focusOn();
				break;
			case 10 :
				popUpObj.focusOn();
				break;
		}
	},
	
	focusOut : function() {
		switch(this.area) {
			case 0 :
				
				break;
			case 1 :
				listObj.focusOut();
				break;
			case 2 :
				
				break;
			case 5 :
				listDoObj.focusOut();
				break;
			case 10 :
				popUpObj.focusOut();
				break;
		}
	},
	
	moveX : function(_num) {
		switch(this.area) {
			case 0 :
				
				break;
			case 1 :
				listObj.focusMove(_num);
				break;
			case 2 :
				
				break;
			case 5 :
				listDoObj.hidden();
				listObj.focusMove(_num);
				break;
			case 10 :
				popUpObj.focusMove(_num);
				break;
		}
	},
	
	moveY : function(_num) {
		switch(this.area) {
			case 0 :
				
				break;
			case 1 :
				
				break;
			case 2 :
				
				break;
			case 5 : 
				listDoObj.focusMove(_num);
				break;
			case 10 :
				
				break;
		}
	},
	
	changePage : function(_num) {
		if(1 == this.area) {
			listObj.changePage(_num);
		}
	},
	
	doSelect : function() {
		ajaxForPanelClick();
		switch(this.area) {
			case 0 :
				
				break;
			case 1 :
				listObj.doSelect();
				break;
			case 2 :
				
				break;
			case 5 : 
				listDoObj.doSelect();
				break;
			case 10 :
				popUpObj.doSelect();
				break;
		}
	},
	
	doBack : function() {
		if(10 == this.area) {
			popUpObj.hidden();
		} else if(5 == this.area) {
			listDoObj.hidden();
		} else {
			location.href = oparateObj.backUrl;
		}
	}
