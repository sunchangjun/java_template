<script type="text/javascript">
var listObj = {
	pos : 0,
	max_size : 6,
	get_flag : false, //表示是否在拿数据中
	cur_size : 0,
	size_line : 6,
	page_pos : 0,
	page_size : 1,
	len : 0,
	focusObj : $("self_ad_focus"),
	dataArray : [],
	
	init : function(_arr) {
		this.dataArray = _arr || this.dataArray;
		this.len = this.dataArray.length;
		
		this.page_size = Math.ceil(this.len / this.max_size);
		if(this.page_pos >= this.page_size) {
			this.page_pos = this.page_size - 1;
		}
		
		this.show();
	},
	
	show : function() {
		if(this.page_pos == this.page_size - 1) {
			if(0 != this.len && 0 == this.len % this.max_size) {
				this.cur_size = this.max_size;
			} else {
				this.cur_size = this.len % this.max_size;
			}
		} else {
			this.cur_size = this.max_size;
		}
		
		var obj;
		var num = this.page_pos * this.max_size;
		for(var i = 0; i < this.max_size; i++) {
			if(i < this.cur_size) {
				obj = this.dataArray[num + i];
				if(oparateObj.type!="top"){
                    this.showText($("list" + i + "_text"), obj.name);
                }
				$("list" + i + "_img").src = (null == obj.bposter || "" == obj.bposter) ? "images/139.png" :obj.bposter;
				$("list" + i).style.visibility = "visible";
			} else {
				$("list" + i).style.visibility = "hidden";
			}
		}
		
		
		if(0 == this.page_pos) {
			$("list_left").style.visibility = "hidden";
		} else {
			$("list_left").style.visibility = "visible";
		}
		
		if(this.page_size - 1 == this.page_pos) {
			$("list_right").style.visibility = "hidden";
		} else {
			$("list_right").style.visibility = "visible";
		}
	},
	
	showText : function(_obj, _text) {
		if(_text.length >  9) {
			_obj.style.lineHeight = "22px";
		} else {
			_obj.style.lineHeight = "46px";
		}
		_obj.innerHTML = _text;
	},
	
	time_out : 0,
	focusOn : function(_flag) {
		var num = this.page_pos * this.max_size + this.pos;
		var obj = this.dataArray[num];
		if(obj) {
            if(oparateObj.type!="top"){
                this.time_out = showTitleForMarquee2(obj.name, $("list_focus_text"), 18);
            }
			$("list_focus_img").src = (null == obj.bposter || "" == obj.bposter) ? "images/139.png" : obj.bposter;
		}
		if(!_flag) {
			this.focusObj.style.visibility = "visible";
			if(checkSTBType()) {
				this.focusObj.style.webkitTransitionDuration = "350ms";
			}
		}
	},
	
	focusOut : function(_flag) {
		clearTimeout(this.time_out);
		var num = this.page_pos * this.max_size + this.pos;
		if(!_flag) {
			this.focusObj.style.visibility = "hidden";
			if(checkSTBType()) {
				this.focusObj.style.webkitTransitionDuration = "0ms";
			}
		}
	},
	
	focusMove : function(_num) {
		var num = _num || 0;
		var pos = this.pos + num;
		if(pos < 0) {
			this.changePage(-1);
		} else if(pos >= this.cur_size) {
			this.changePage(1);
		} else {
			this.focusOut(1);
			this.pos = pos;
			
			$("list_focus").style.left = 77 + 186 * this.pos + "px";
			this.focusObj.style.left = 47 + 186 * this.pos + "px";
			this.focusOn(1);
			listDoObj.init();
		}
	},
	
	changePage : function(_num) {
		var num = _num || 0;
		var pos = this.page_pos + num;
		if(pos >= 0 && pos < this.page_size) {
			this.pos = 0;
			this.page_pos = pos;
			
			this.show();
			this.focusMove();
		}
	},
	
	doSelect : function() {
		var num = this.page_pos * this.max_size + this.pos;
		var obj = this.dataArray[num];
		var backUrl = window.location.href.split("?")[0] + "?area=" + oparateObj.area + "&l_p_pos=" + this.page_pos + "&l_pos=" + this.pos
            + "&rid=" + oparateObj.rid + "&title=" + Q.encode(oparateObj.title)+"&test="+ paramObj.test
            + "&code=" + oparateObj.code + "&backUrl=" + Q.encode(oparateObj.backUrl)
            +"&type="+oparateObj.type+"&img="+Q.encode(oparateObj.img)+"&prid="+paramObj.prid+"&wthxpath="+Q.encode(paramObj.wthxpath);
		if(oparateObj.type=="top"){
            var url = Q.getNewAddUrl("list.htm")+"backUrl="+Q.encode(backUrl)+"&rid="+obj.rid+"&name="+Q.encode(obj.name)+"&prid="+paramObj.prid
                +"&test="+ paramObj.test+"&code=top"+"&wthxpath="+Q.encode(paramObj.wthxpath+"|"+obj.rid);
            window.location.href=url;
        }else{
            ajaxForAuth(obj,backUrl,function(authResult){
                checkAndToPlay(authResult,backUrl,"cate",oparateObj.rid,paramObj.prid,paramObj.test,paramObj.wthxpath);
            },function () {
                
            },paramObj.wthxpath)
        }
	}
};

var listDoObj = {
	pos : 0,
	size : 2,
	rid : "",
	collect : false,
	doFuc : null,
	focus_id : "list",
	
	init : function(_focus_id) {
		this.focus_id = _focus_id || this.focus_id;
		this.pos = 0;
		this.focusMove();
	},
	
	visible : function(_doFuc) {
		this.doFuc = _doFuc || null;
		oparateObj.area = 5;
		$(this.focus_id + "_do").style.display = "block";
	},
	
	hidden : function() {
		oparateObj.area = 1;
		$(this.focus_id + "_do").style.display = "none";
	},
	
	initCollectMS : function(_collect, _flag) {
		var flag = _flag || 0;
		this.collect = _collect || false;
		if(this.collect) {
			$(this.focus_id + "_do_btn1_0").style.visibility = "hidden";
			$(this.focus_id + "_do_btn1_1").style.visibility = "visible";
		} else {
			$(this.focus_id + "_do_btn1_0").style.visibility = "visible";
			$(this.focus_id + "_do_btn1_1").style.visibility = "hidden";
		}
	},
	
	focusOn : function(_flag) {
		
	},
	
	focusOut : function(_flag) {
		
	},
	
	focusMove : function(_num) {
		var num = _num || 0;
		var pos = this.pos + num;
		if(pos >= 0 && pos < this.size) {
			this.focusOut(1);
			this.pos = pos;
			
			$(this.focus_id + "_do_focus").style.top = 21 + 55 * this.pos + "px";
			this.focusOn(1);
		}
	},
	
	doSelect : function(_num) {
		if(0 == this.pos) {
			if(this.doFuc) {
				this.doFuc();
			}
		} else if(1 == this.pos) {
			if(this.collect) {
				ajaxForRemoveCollect(this.id);
			} else {
				ajaxForAddCollect(this.id);
			}
		}
	}
};

